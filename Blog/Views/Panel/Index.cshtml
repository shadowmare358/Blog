@model IEnumerable<Post>
@{ 

}
<h2>Admin Panel</h2>
<a asp-controller="Panel" asp-action="Edit">
    Add New Post
</a>
<div>
    <img src="~/content/static/p-40III.png" width="500" height="500" />
    <ul>
        @foreach (var post in Model)
        {
            <li>
                @post.Title
                -
                <a asp-controller="Panel" asp-action="Edit" asp-route-id="@post.Id">
                    Edit
                </a>
                -
                <a asp-controller="Panel" asp-action="Edit" asp-route-id="@post.Id">
                    Delete
                </a>
            </li>
        }
    </ul>
</div>